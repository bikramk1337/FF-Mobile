{
  "expo": {
    "name": "Fauna Finder",
    "slug": "FaunaAPP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "faunaapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6f6fff"
    },
    "android": {
      "package": "com.yourcompany.faunaapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "faunaapp",
            "host": "reset-password",
            "pathPrefix": "/"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.yourcompany.faunaapp",
      "associatedDomains": [
        "applinks:yourdomain.com"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "f0c28de9-7769-4020-bd68-ba42b677f6f9"
      }
    }
  }
}
